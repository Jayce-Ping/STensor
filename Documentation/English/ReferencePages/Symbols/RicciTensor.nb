Cell["\<\
The Ricci Tensor is defined by a kind of self-contract of Riemann Tensor:\
\>", "ExampleText",
 CellChangeTimes->{{3.8953766449657974`*^9, 3.8953766634399166`*^9}},
 CellID->958343587,ExpressionUUID->"dfbe6a54-5ce2-4343-bc69-e44852b178b1"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"g", "=", 
   RowBox[{"CreateTensor", "[", 
    RowBox[{"\"\<g\>\"", ",", 
     RowBox[{"{", 
      RowBox[{"\[Theta]", ",", "\[Phi]"}], "}"}], ",", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"R", "^", "2"}], ",", 
        RowBox[{
         RowBox[{"R", "^", "2"}], 
         RowBox[{
          RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}]}]}], "}"}], "]"}],
      ",", 
     RowBox[{"IsMetric", "->", "True"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RicciTensor", "[", "g", "]"}], "[", "\"\<Components\>\"", "]"}], 
  " ", "==", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"RiemannTensor", "[", "g", "]"}], "[", 
    RowBox[{"\"\<a\>\"", ",", "\"\<bac\>\""}], "]"}], "[", 
   "\"\<Components\>\"", "]"}]}]}], "Input",
