Cell[BoxData[{
 RowBox[{
  RowBox[{"metric", "=", 
   RowBox[{"CreateTensor", "[", 
    RowBox[{"\"\<g\>\"", ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "2"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Theta]", ",", "\[Phi]"}], "}"}], ",", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"R", "^", "2"}], ",", 
        RowBox[{
         RowBox[{"R", "^", "2"}], 
         RowBox[{
          RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}]}]}], "}"}], "]"}],
      ",", 
     RowBox[{"IsMetric", "->", "True"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"ChristoffelSymbol", "[", "metric", "]"}], "[", "\"\<Symbol\>\"", 
  "]"}]}], "Input",
